{
  "name": "umi-test",
  "version": "1.0.0",
  "description": "Scaffold for umi based project",
  "scripts": {
    "start": "umi dev",
    "build": "umi build",
    "serve": "node server --title=umi-test"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": "eslint --max-warnings=0 --ignore-pattern !.umirc.js"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-angular"
    ]
  },
  "dependencies": {
    "axios": "^0.21.1",
    "classnames": "^2.2.6",
    "dva": "^2",
    "dva-core": "^1",
    "express": "^4.17.1",
    "js-cookie": "^2.2.1",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "path-to-regexp": "^3.1.0",
    "rc-form": "^2.4.9",
    "react": "^16.11.0",
    "react-redux": "^7.1.1",
    "replace-in-file": "^4.2.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.2.0",
    "@commitlint/config-angular": "^8.2.0",
    "@typescript-eslint/parser": "^1",
    "babel-eslint": "^10.0.3",
    "babel-plugin-import": "^1.12.2",
    "eslint": "^5",
    "eslint-config-prettier": "^6.5.0",
    "eslint-config-umi": "^1.5.0",
    "eslint-plugin-flowtype": "^2",
    "eslint-plugin-import": "^2",
    "eslint-plugin-jsx-a11y": "^6",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7",
    "eslint-plugin-react-hooks": "1.5.0",
    "husky": "^3.0.9",
    "less-vars-to-js": "^1.3.0",
    "lint-staged": "^9.4.2",
    "prettier": "^1.18.2",
    "redbox-react": "^1",
    "umi": "^2.10.7",
    "umi-plugin-react": "^1.12.8"
  }
}
